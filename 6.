from turtle import *
screensize(1000, 1000)
tracer(10)
n = 15
down()
for i in range(2):
    forward(5 * n)
    right(90)
    forward(8 * n)
    right(90)
up()
forward(2 * n)
right(90)
forward(4 * n)
left(90)
down()
for i in range(2):
    forward(4 * n)
    right(90)
    forward(9 * n)
    right(90)
up()
tracer(100)
for x in range(-100, 100):
    for y in range(-100, 100):
        goto(x * n, y * n)
        dot(5, 'red')
done()
